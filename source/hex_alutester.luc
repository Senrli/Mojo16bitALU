              alu_simple.alufn = 6h000;
              
            state_auto.B_VAL_DISP:
              state = 4;
              data_bus_out = B;
              state_auto.d = state_auto.ADD_1;
              alu_simple.alufn = 6h000;
              
            state_auto.ADD_1:
              state = 5;
              true_lut.state = 5;
              alu_simple.a = true_lut.a;
              alu_simple.b = true_lut.b;
              alu_simple.alufn = 6h000;
