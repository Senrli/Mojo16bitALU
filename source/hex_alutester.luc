      fsm state_manual = {A, A_DISP, B, B_DISP, ALU, ALU_DISP, RESULT, RESULT_DISP};
      
    }
    
  }
  
  alu_simple alu_simple;
  true_lut true_lut;
  
  const A = 16hdead; 
  const B = 16hbeef;
  sig state[5];
  

  always {
  
    case (state_main.q){
      state_main.IDLE:
        state = 0;
        data_bus_out = 19x{0};
        alu_simple.alufn = 6h000;
        alu_simple.a = 16h0;
        alu_simple.b = 16h0;
        if (auto){
          state_main.d = state_main.AUTO;
          delay_counter.d = 0; 
          pass_counter.d = 0;         
        } else if (auto_fail){
          state_main.d = state_main.AUTO_FAIL;
          delay_counter.d = 0;
          pass_counter.d = 0;
        } else if (manual){
          state_main.d = state_main.MANUAL;
          delay_counter.d = 0;
        }
      state_main.AUTO:
        alu_simple.a = true_lut.a;
        alu_simple.b = true_lut.b;
        
        if (delay_counter.q == 49999999) {
          delay_counter.d = 0;
          case(state_auto.q){
            state_auto.A_VAL:
              state = 1;
              data_bus_out = A;
              state_auto.d = state_auto.A_VAL_DISP;
              alu_simple.alufn = 6h000;
             
