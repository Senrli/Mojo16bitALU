              pass_counter.d = pass_counter.q + ((&(~(true_lut.z ^ alu_simple.z))) & (&(~(true_lut.v ^ alu_simple.v))) & (&(~(true_lut.n ^ alu_simple.n))) & (&(~(alu_simple.alu ^ true_lut.alu_lut))));
